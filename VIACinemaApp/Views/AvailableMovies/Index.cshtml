@{
    ViewData["Title"] = "Billboard";
}
<environment include="Development">
    <script src="~/js/site.js" type="text/javascript "> </script>
    <link rel="stylesheet" href="~/css/availableMovies.css" media="screen" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
</environment>

<h2 id="logo">Billboard</h2>

<div id="tab"></div>

<div id="tabContainer"></div>
<h1>Search by Date</h1>
<p>
    Date: <input type="date" name="id" id="date" placeholder="Please select a date">
    <button id="submit" onclick="getMovieByDate()">Search</button>
</p>
<div id="movies"> </div>